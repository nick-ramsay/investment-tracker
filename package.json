{
  "name": "investment-tracker",
  "version": "1.0.0",
  "engines": {
    "node": "12.x"
  },
  "description": "Application for managing functions, including messages, from various outside applications",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nick-ramsay/investment-tracker.git"
  },
  "author": "Nick Ramsay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nick-ramsay/investment-tracker/issues"
  },
  "homepage": "https://github.com/nick-ramsay/investment-tracker#readme",
  "dependencies": {
    "@sendgrid/mail": "^7.4.0",
    "axios": "^0.21.0",
    "cheerio": "^1.0.0-rc.5",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "core-js": "^3.8.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "googleapis": "^65.0.0",
    "if-env": "^1.0.4",
    "js-sha256": "^0.9.0",
    "moment": "^2.29.1",
    "mongoose": "^5.11.9",
    "nodemailer": "^6.4.16",
    "nodemon": "^2.0.6",
    "p-limit": "^3.1.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  }
}
